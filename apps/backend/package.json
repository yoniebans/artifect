{
  "name": "@artifect/backend",
  "version": "0.1.0",
  "description": "AI-Assisted Artifact Engineering Backend",
  "keywords": [
    "artificial",
    "architect",
    "artif-ect",
    "artifact",
    "ai"
  ],
  "author": "yoniebans",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "clean": "rimraf dist node_modules",
    "test": "dotenv -e .env.test -- jest --config=jest.config.js",
    "test:watch": "dotenv -e .env.test -- jest --watch --config=jest.config.js",
    "test:cov": "dotenv -e .env.test -- jest --coverage --config=jest.config.js",
    "test:debug": "dotenv -e .env.test -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config=jest.config.js",
    "test:db": "dotenv -e .env.test -- jest src/database --runInBand --config=jest.config.js",
    "test:config": "dotenv -e .env.test -- jest src/config --runInBand --config=jest.config.js",
    "test:app": "dotenv -e .env.test -- jest src/app --runInBand --config=jest.config.js",
    "test:repositories": "dotenv -e .env.test -- jest src/repositories --runInBand --config=jest.config.js",
    "test:ai": "dotenv -e .env.test -- jest src/ai --runInBand --config=jest.config.js",
    "test:workflow": "dotenv -e .env.test -- jest src/workflow --runInBand --config=jest.config.js",
    "test:context": "dotenv -e .env.test -- jest src/context --runInBand --config=jest.config.js",
    "test:api": "dotenv -e .env.test -- jest src/api --runInBand --config=jest.config.js",
    "test:auth": "dotenv -e .env.test -- jest src/auth --runInBand --config=jest.config.js",
    "test:services": "dotenv -e .env.test -- jest src/services --runInBand --config=jest.config.js",
    "test:templates": "dotenv -e .env.test -- jest src/templates --runInBand --config=jest.config.js",
    "test:integration": "dotenv -e .env.test -- jest \".*\\.integration\\.spec\\.ts$\" --runInBand --config=jest.config.js",
    "test:integration:api": "dotenv -e .env.test -- jest src/api/\".*\\.integration\\.spec\\.ts$\" --runInBand --config=jest.config.js",
    "test:integration:context": "dotenv -e .env.test -- jest src/context/\".*\\.integration\\.spec\\.ts$\" --runInBand --config=jest.config.js",
    "test:integration:repositories": "dotenv -e .env.test -- jest src/repositories/\".*\\.integration\\.spec\\.ts$\" --runInBand --config=jest.config.js",
    "test:integration:templates": "dotenv -e .env.test -- jest src/templates/\".*\\.integration\\.spec\\.ts$\" --runInBand --config=jest.config.js",
    "test:integration:workflow": "dotenv -e .env.test -- jest src/workflow/\".*\\.integration\\.spec\\.ts$\" --runInBand --config=jest.config.js",
    "test:integration:auth": "dotenv -e .env.test -- jest src/auth/\".*\\.integration\\.spec\\.ts$\" --runInBand --config=jest.config.js",
    "test:all": "dotenv -e .env.test -- jest --runInBand --config=jest.config.js",
    "test:e2e": "dotenv -e .env.test -- jest --config=./test/jest-e2e.json --runInBand",
    "test:repositories:e2e": "dotenv -e .env.test -- jest --config=./test/jest-e2e.json \"repositories.*\\.e2e-spec\\.ts$\" --runInBand",
    "test:complete:e2e": "dotenv -e .env.test -- jest --config=./test/jest-e2e.json \"complete.*\\.e2e-spec\\.ts$\" --runInBand",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:reset": "ts-node prisma/reset.ts",
    "db:reset:seed": "ts-node prisma/reset.ts && npm run db:seed"
  },
  "dependencies": {
    "@artifect/shared": "workspace:*",
    "@clerk/backend": "^1.25.5",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "6.3.0",
    "@prisma/client": "^4.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.7",
    "handlebars": "^4.7.8",
    "joi": "^17.9.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.13",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "prisma": "^4.12.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.4",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  }
}